{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "UsuarioUrl": "https://ppgm-api-usuario:5201",
  "SaemUrl": "https://ppgm-api-saem:5301",
  "SafimUrl": "https://ppgm-api-safim:5401",
  "SasciUrl": "https://ppgm-api-sasci:5501",
  "SturUrl": "https://ppgm-api-stur:5601",
  "ConnectionStrings": {
    "RedisConnection": "ppgm-redis,port: 6379,password=@ppgm2021"
  },
  "MessageQueueConnection": {
    "MessageBus": "host=ppgm-rabbit:5672;publisherConfirms=true;timeout=30;username=ppgm;password=ppgm"
  },
  "AppSettings": {
    "AutenticacaoJwksUrl": "https://ppgm-api-autenticacao:5101/jwks"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "LevelSwitches": { "$controlSwitch": "Information" },
    "WriteTo": [
      {
        "Name": "MongoDBCapped",
        "Args": {
          "databaseUrl": "mongodb://root:ppgm2021@localhost/ppgm-db",
          "collectionName": "log",
          "cappedMaxSizeMb": "50",
          "cappedMaxDocuments": "1000"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithExceptionDetails" ],
    "Properties": {
      "Environment": "LocalDev"
    }
  }
}