    "LogConnection": "mongodb://root:@ppgm2021==@localhost:27017/ppgm-db"
  },
  "MessageQueueConnection": {
    "MessageBus": "host=localhost:5672;publisherConfirms=true;timeout=10"
  },
  "AppSettings": {
    "AutenticacaoJwksUrl": "https://localhost:5101/jwks"
  },
  "Serilog": {
